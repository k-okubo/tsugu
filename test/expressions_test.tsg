// RUN: cat %s | %tsugu | FileCheck %s
// CHECK: result = 1

def assert_eq(expected, actual) {
  if (expected == actual) {
    1;
  } else {
    0;
  };
}

def assert(cond) {
  if (cond) {
    1;
  } else {
    0;
  };
}

def main() {
  1
  * assert_eq(3, 1 + 2)
  * assert_eq(2, 5 - 3)
  * assert_eq(6, 2 * 3)
  * assert_eq(2, 5 / 2)
  * assert_eq(9, (1 + 2) * 3)
  * assert_eq(7, 1 + 2 * 3)
  * assert(1 == 1)
  * assert(1 < 2)
  * assert(3 > 2)
  ;
}
