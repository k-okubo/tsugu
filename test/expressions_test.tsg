// RUN: cat %s | %tsugu | FileCheck %s
// CHECK: result = 1

def asserteq(expected, actual) {
  if (expected == actual) {
    1;
  } else {
    0;
  };
}

def main() {
  1
  * asserteq(3, 1 + 2)
  * asserteq(2, 5 - 3)
  * asserteq(6, 2 * 3)
  * asserteq(2, 5 / 2)
  * asserteq(9, (1 + 2) * 3)
  * asserteq(7, 1 + 2 * 3)
  ;
}
